<body>
	<div class="navbar navbar-inverse">

		<div class="container">

			<div class="navbar-header">
				<a class="navbar-brand" href="../index.html"> <span
					class="glyphicon glyphicon-home"></span> Home
				</a>
			</div>

			<div id="navbar" class="collapse navbar-collapse">

				<ul class="nav navbar-nav navbar-right">
#foreach( $entity in $selectedEntities )
                   	<li><a href="../${entity.name}/${entity.name}List.html">${entity.name}</a></li>
#end
				</ul>
			</div>
		</div>
	</div>

	<br />

    <div class="container">
        <form class="form-horizontal" method="post">
            <div class="panel-group">
                <div class="panel panel-info">
                    <div class="panel-heading">
						<h3>
							$entity.name Form

                            <a href="./${entity.name}List.html">
                                <button type="button" class="btn btn-link pull-right">
                                    <span class="glyphicon glyphicon-circle-arrow-left" ></span> List
                                </button>
                            </a>

							<!-- visible only if we update -->
							<a href="./${entity.name}Form.html"
								title="Create a new entity">
								<button type="button" class="btn btn-success pull-right">
									<span class="glyphicon glyphicon-plus" ></span> Create
								</button>
							</a>
						</h3>
					</div>


                    <div class="panel-body">
#foreach( $field in $entity.attributes )
                        <div class="form-group">
                            <label for="$field.name" class="col-sm-2">$field.name#if($field.isDatabaseNotNull()) *#end</label>
#if( $field.isFK() )
							<div class="col-sm-8">
                                <select id='$field.name' class="selectpicker">
                                	<option></option>
                                </select>
                                <a href="../${field.referencedEntityName}/${field.referencedEntityName}Form.html">
                                    <button type="button" class="btn btn-default"><span class="glyphicon glyphicon-new-window"></span></button>
                                </a>
                            </div>
#else
                            <div class="col-sm-8">
                                <input id="$field.name" class="form-control" name='$field.name' />
                            </div>
#end
                        </div>
#end
						<div class="panel-footer">
							<a href="./${entity.name}Form.html">
								<button type="button" class="btn btn-danger pull-right"
									onClick="confirm('Do you really want to delete this entity ?')">
									<span class="glyphicon glyphicon-trash"></span> Delete
								</button>
							</a> 
							<a href="./${entity.name}Form.html">
								<button type="submit" class="btn btn-success">
									<span class="glyphicon glyphicon-floppy-disk"></span> Save
								</button>
							</a>
							<div class="clearfix"></div>
						</div>
                	</div>
                </div>
            </div>
        </form>
    </div>
</body>